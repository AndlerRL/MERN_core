{
  "name": "mern_basics",
  "description": "A MERN stack core structure",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10.18.0"
  },
  "watch": {
    "test": {
      "patterns": [
        "server/**",
        "src/**"
      ],
      "extensions": "js,jsx",
      "quiet": true,
      "delay": 3000
    }
  },
  "author": {
    "name": "Roberto Lucas",
    "email": "andre.rlucas@outlook.com",
    "url": "https://andler.netlify.com"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "log4js": "^6.1.0",
    "mongoose": "^5.8.3",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^11.2.7",
    "react-router-dom": "^5.1.2",
    "rebass": "^4.0.7",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start-react": "react-scripts start",
    "start": "node server/server.js",
    "test": "jest --env=jsdom --colors",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "deploy": "npm run build; cf push",
    "dev": "cross-env NODE_ENV=development nf --procfile Procfile-dev --port 1313 start",
    "dev:server": "nodemon server/server.js",
    "lint": "eslint --ext .js --ext .jsx --color .",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "prebuild": "npm run test",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "foreman": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "npm-watch": "^0.6.0",
    "react-scripts": "^3.3.0",
    "sinon": "^8.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/config/setupTests.js",
      "<rootDir>/server/tests/config/setupTests.js"
    ]
  }
}
